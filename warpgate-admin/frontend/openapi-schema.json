{
  "openapi": "3.0.0",
  "info": {
    "title": "warpgate-admin",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "paths": {
    "/sessions": {
      "get": {
        "operationId": "api_get_all_sessions",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SessionSnapshot"
                  }
                }
              }
            }
          },
          "404": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmptyResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "api_close_all_sessions",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmptyResponse"
                }
              }
            }
          }
        }
      }
    },
    "/sessions/{id}": {
      "get": {
        "operationId": "api_get_session",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/Uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SessionSnapshot"
                }
              }
            }
          },
          "404": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmptyResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SessionSnapshot": {
        "type": "object",
        "required": [
          "id",
          "started"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "user": {
            "allOf": [
              {
                "$ref": "#/components/schemas/UserSnapshot"
              }
            ],
            "nullable": true
          },
          "target": {
            "allOf": [
              {
                "$ref": "#/components/schemas/TargetSnapshot"
              }
            ],
            "nullable": true
          },
          "started": {
            "type": "string",
            "format": "partial-date-time"
          },
          "ended": {
            "type": "string",
            "format": "partial-date-time",
            "nullable": true
          }
        }
      },
      "UserSnapshot": {
        "type": "object",
        "required": [
          "username"
        ],
        "properties": {
          "username": {
            "type": "string"
          }
        }
      },
      "TargetSnapshot": {
        "type": "object",
        "required": [
          "host",
          "port"
        ],
        "properties": {
          "host": {
            "type": "string"
          },
          "port": {
            "type": "integer",
            "format": "uint16",
            "minimum": 0.0
          }
        }
      },
      "EmptyResponse": {
        "type": "object"
      },
      "Uuid": {
        "type": "string",
        "format": "uuid"
      }
    }
  }
}