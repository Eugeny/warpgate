{
  "openapi": "3.0.0",
  "info": {
    "title": "warpgate-admin",
    "version": "0.1.0"
  },
  "paths": {
    "/sessions": {
      "get": {
        "operationId": "api_get_all_sessions",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SessionData"
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "api_close_all_sessions",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmptyResponse"
                }
              }
            }
          }
        }
      }
    },
    "/sessions/{id}": {
      "get": {
        "operationId": "api_get_session",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SessionData"
                }
              }
            }
          },
          "404": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmptyResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SessionData": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "user": {
            "allOf": [
              {
                "$ref": "#/components/schemas/UserData"
              }
            ],
            "nullable": true
          },
          "target": {
            "allOf": [
              {
                "$ref": "#/components/schemas/TargetData"
              }
            ],
            "nullable": true
          }
        }
      },
      "UserData": {
        "type": "object",
        "required": [
          "username"
        ],
        "properties": {
          "username": {
            "type": "string"
          }
        }
      },
      "TargetData": {
        "type": "object",
        "required": [
          "host",
          "port"
        ],
        "properties": {
          "host": {
            "type": "string"
          },
          "port": {
            "type": "integer",
            "format": "uint16",
            "minimum": 0.0
          }
        }
      },
      "EmptyResponse": {
        "type": "object"
      }
    }
  }
}